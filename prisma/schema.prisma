generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model data {
  id                 Int     @id @default(autoincrement())
  raw                String?
  title              String?
  tg                 String?
  initial_proposal   String?
  author             String?
  course             String?
  advisor            String?
  co_Advisor         String?
  possible_appraiser String?
  proposal_abstract  String?
  day                String?
  hour               String?
  evaluation_panel   String?
  local              String?
  key_words          String?
  semester           String?
  area               String?
  final_score        String?
}
