generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model data {
  id                 Int     @id @default(autoincrement())
  raw                String
  title              String
  tg                 String?
  Initial_proposal   String
  Author             String
  Course             String
  Advisor            String
  Co_Advisor         String?
  Possible_appraiser String?
  Proposal_abstract  String
  date               String
  evaluation_panel   String
  local              String?
  key_words          String?
  semester           String
}
