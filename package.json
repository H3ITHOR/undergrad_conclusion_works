{
  "name": "undergrad_conclusion_works",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts --watch",
    "dev:watch": "nodemon --exec ts-node src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "docker:up": "docker-compose -f ucw-c.yml up -d",
    "docker:down": "docker-compose -f ucw-c.yml down",
    "setup": "npm install && npm run db:generate && npm run docker:up && npm run db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/H3ITHOR/undergrad_conclusion_works.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/H3ITHOR/undergrad_conclusion_works/issues"
  },
  "homepage": "https://github.com/H3ITHOR/undergrad_conclusion_works#readme",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "prettier": "^3.6.2",
    "prisma": "^6.11.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "express": "^5.1.0"
  }
}
